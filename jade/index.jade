                        h1 Gliederung
                        - var sectionlist = ["Intro", "HTML5", "Neue Dinge", "Verlorene Dinge", "DOM", "Ids in HTML", "Klassen in HTML"]
                        - var i = 1
                        each section in sectionlist
                          - var g = "#g-";
                          li(id="a-" + i)
                            a(href=g + i)= section
                            - i++

                
                section#g-1
                    h1.sectionheading Agenda
                    ol
                        li HTML5 
                        li CSS
                        li JQuery
                        li Grobe Struktur
                        li Detailierter Aufbau
                          ol
                            li JavaScript
                            li Header
                            li Hauptinhalt
                            li Footer
                            li Sections
                        
