    .content#page4
            .container
                .gliederung
                  ol
                      h1 Gliederung
                      - var sectionlist = ["Grundstruktur Index","Grundstruktur", "Schreibstil", "Dateiordnung"]
                      - var i = 1
                      each section in sectionlist
                        - var g = "#g-";
                        li(id="a-" + i)
                          a(href=g + i)= section
                          - i++ 

                section#g-1
                  h1.sectionheading Grundstruktur Index
                  h2 Desktop
                  .showcontainer.desktop
                    .url
                    .sitecontainer
                      .wrapper
                        .bigheader Azubi-Welten
                        .col Flexitem
                          .bigsquare
                            .smallsquare
                            .text
                        .col Flexitem
                          .bigsquare
                            .smallsquare  
                            .text
                  h2 Mobile
                  .showcontainer.mobile
                    .url
                    .sitecontainer
                      .wrapper
                        .bigheader Azubi-Welten
                        .col
                          .bigsquare
                            .smallsquare
                            .text
                        .col
                          .bigsquare
                            .smallsquare  
                            .text
                section#g-2
                  h1.sectionheading Grundstruktur
                  h2 Desktop
                  .colbuttons
                    a.big(href="azubisite/site.html" target="_blank") Showsite
                    a.big(href="azubisite/sitenocss.html" target="_blank") Showsite No CSS
                    a.big(href="site_grob.html") Erkl√§rung
                    .clearfix
                section#g-3 
                  h1.sectionheading Schreibstil
                  h2 HTML: IDs und Klassen
                  .codebox
                    pre(class="brush: xml").
                      &lt;div class="maincol"&gt;
                      
                      &lt;p class="sb_news-p-short sb_news-display"&gt;
                      
                      &lt;div class="button js_news-more"&gt;
                      
                      &lt;div id="nav_siebenBerge" class="sb_news"&gt;
                      
                      &lt;div id="js_progress_trigger" class="slider"&gt;
                  h2 CSS
                  .codebox
                    pre(class="brush: css").
                      /* Viele rem und em */
                      h1{ font-size: 3rem; }
                        
                  h2 JavaScript
                  .codebox
                    pre(class="brush: js").
